0019FC 0F59FC                                                 ORG     NUM_CODE_START, NUM_CODE_START_LIN
                                      ;#Reverse unsigned double word
                                      ; args:   Y:X: unsigned double value
                                      ;         B:   base   (2<=base<=16)
                                      ; result: A:   number of digits
                                      ;         SP+0: MSB
                                      ;         SP+1:  |
                                      ;         SP+2:  |reverse
                                      ;         SP+3:  |number
                                      ;         SP+4:  |
                                      ;         SP+5: LSB
                                      ; SSTACK: 18 bytes
                                      ;         X, Y and B are preserved
0019FC 0F59FC -> $19FC                NUM_REVERSE             EQU     *
                                      ;Stack layout:
0019FC 0F59FC -> $0000                NUM_REVERSE_FHW         EQU     $00 ;SP+ 0: MSB
                                                                          ;SP+ 1:  |forward
0019FC 0F59FC -> $0002                NUM_REVERSE_FLW         EQU     $02 ;SP+ 2:  |number
                                                                          ;SP+ 3: LSB
0019FC 0F59FC -> $0004                NUM_REVERSE_COUNT       EQU     $04 ;SP+ 4: count -> A
0019FC 0F59FC -> $0005                NUM_REVERSE_BASE        EQU     $05 ;SP+ 5: base  -> B
0019FC 0F59FC -> $0006                NUM_REVERSE_Y           EQU     $06 ;SP+ 6: +Y
                                                                          ;SP+ 7: +
0019FC 0F59FC -> $0008                NUM_REVERSE_X           EQU     $08 ;SP+ 8: +X
                                                                          ;SP+ 9: +
0019FC 0F59FC -> $000A                NUM_REVERSE_RTN         EQU     $0A ;SP+10: +return address
                                                                          ;SP+11: +
0019FC 0F59FC -> $000C                NUM_REVERSE_RHW         EQU     $0C ;SP+12: MSB
                                                                          ;SP+13:  |
0019FC 0F59FC -> $000E                NUM_REVERSE_RMW         EQU     $0E ;SP+14:  |reverse
                                                                          ;SP+15:  |number
0019FC 0F59FC -> $0010                NUM_REVERSE_RLW         EQU     $10 ;SP+16:  |      +return address at
                                                                          ;SP+17: LSB     +subroutine entry
                                                              ;Setup stack (double value in Y:X, base in B)
0019FC 0F59FC 87                                              CLRA
0019FD 0F59FD 18 02 80 AA                                     MOVW    0,SP, 6,-SP             ;move return address to SP+10
001A01 0F5A01 6C 86                                           STD     6,SP                    ;initialize reverse number
001A03 0F5A03 18 00 84 00 00                                  MOVW    #$0000, 4,SP            ;reverse number = base
001A08 0F5A08 18 00 82 00 00                                  MOVW    #$0000, 2,SP
001A0D 0F5A0D 34                                              PSHX                            ;store X at SP+8
001A0E 0F5A0E 35                                              PSHY                            ;store Y at SP+6
001A0F 0F5A0F 3B                                              PSHD                            ;store count:base at SP+4
001A10 0F5A10 34                                              PSHX                            ;store double value at SP+0
001A11 0F5A11 35                                              PSHY
                                                              ;Divide FHW by base
001A12 0F5A12 EE 80                   NUM_REVERSE_1           LDX     NUM_REVERSE_FHW,SP      ;FHW => X
001A14 0F5A14 27 0B                                           BEQ     NUM_REVERSE_2           ;skip division step
001A16 0F5A16 87                                              CLRA                            ;base => D
001A17 0F5A17 E6 85                                           LDAB    NUM_REVERSE_BASE,SP
001A19 0F5A19 B7 D4                                           EXG     X, D
001A1B 0F5A1B 18 10                                           IDIV                            ;D / X => X,  D % X => D
001A1D 0F5A1D 6E 80                                           STX     NUM_REVERSE_FHW,SP      ;result => FHW
                                                              ;Divide FLW by base (prev. remainder in D)
001A1F 0F5A1F B7 45                                           TFR     D, X                    ;remainder => X
001A21 0F5A21 87                      NUM_REVERSE_2           CLRA                            ;base => D
001A22 0F5A22 E6 85                                           LDAB    NUM_REVERSE_BASE,SP
001A24 0F5A24 ED 82                                           LDY     NUM_REVERSE_FLW,SP      ;FLW => Y
001A26 0F5A26 B7 D6                                           EXG     X, Y
001A28 0F5A28 B7 D4                                           EXG     X, D
001A2A 0F5A2A 11                                              EDIV                            ;Y:D / X => Y,  Y:D % X => D
001A2B 0F5A2B 6D 82                                           STY     NUM_REVERSE_FLW,SP      ;result => FLW
                                                              ;Add remainder to the reverse value (prev. remainder in D)
001A2D 0F5A2D E3 F0 10                                        ADDD    NUM_REVERSE_RLW,SP      ;RLW
001A30 0F5A30 6C F0 10                                        STD     NUM_REVERSE_RLW,SP
001A33 0F5A33 EC 8E                                           LDD     NUM_REVERSE_RMW,SP      ;RMW
001A35 0F5A35 C9 00                                           ADCB    #$00
001A37 0F5A37 89 00                                           ADCA    #$00
001A39 0F5A39 6C 8E                                           STD     NUM_REVERSE_RMW,SP
001A3B 0F5A3B EC 8C                                           LDD     NUM_REVERSE_RHW,SP      ;RHW
001A3D 0F5A3D C9 00                                           ADCB    #$00
001A3F 0F5A3F 89 00                                           ADCA    #$00
001A41 0F5A41 6C 8C                                           STD     NUM_REVERSE_RHW,SP
                                                              ;Increment digit count
001A43 0F5A43 62 84                                           INC     NUM_REVERSE_COUNT,SP
                                                              ;Check if the calculation is finished
001A45 0F5A45 EC 82                                           LDD     NUM_REVERSE_FLW,SP
001A47 0F5A47 26 04                                           BNE     <NUM_REVERSE_3          ;reverse value incomplete
001A49 0F5A49 EC 80                                           LDD     NUM_REVERSE_FHW,SP
001A4B 0F5A4B 27 2C                                           BEQ     <NUM_REVERSE_4          ;reverse value has been generated
                                                              ;Multiply RLW by base
001A4D 0F5A4D ED F0 10                NUM_REVERSE_3           LDY     NUM_REVERSE_RLW,SP
001A50 0F5A50 87                                              CLRA
001A51 0F5A51 E6 85                                           LDAB    NUM_REVERSE_BASE,SP
001A53 0F5A53 13                                              EMUL                            ;Y * D => Y:D
001A54 0F5A54 6C F0 10                                        STD     NUM_REVERSE_RLW,SP
                                                              ;Multiply RMW by base (carry-over in Y)
001A57 0F5A57 EC 8E                                           LDD     NUM_REVERSE_RMW,SP
001A59 0F5A59 B7 C6                                           EXG     D, Y
001A5B 0F5A5B 6C 8E                                           STD     NUM_REVERSE_RMW,SP
001A5D 0F5A5D 87                                              CLRA
001A5E 0F5A5E E6 85                                           LDAB    NUM_REVERSE_BASE,SP
001A60 0F5A60 13                                              EMUL                            ;Y * D => Y:D
001A61 0F5A61 E3 8E                                           ADDD    NUM_REVERSE_RMW,SP
001A63 0F5A63 6C 8E                                           STD     NUM_REVERSE_RMW,SP
001A65 0F5A65 B7 64                                           TFR     Y, D
001A67 0F5A67 C9 00                                           ADCB    #$00
001A69 0F5A69 89 00                                           ADCA    #$00
                                                              ;Multiply RHW by base (carry-over in D)
001A6B 0F5A6B ED 8C                                           LDY     NUM_REVERSE_RHW,SP
001A6D 0F5A6D 6C 8C                                           STD     NUM_REVERSE_RHW,SP
001A6F 0F5A6F 87                                              CLRA
001A70 0F5A70 E6 85                                           LDAB    NUM_REVERSE_BASE,SP
001A72 0F5A72 13                                              EMUL                            ;Y * D => Y:D
001A73 0F5A73 E3 8C                                           ADDD    NUM_REVERSE_RHW,SP
001A75 0F5A75 6C 8C                                           STD     NUM_REVERSE_RHW,SP
                                                              ;Start new iteration
001A77 0F5A77 20 99                                           JOB     NUM_REVERSE_1
                                                              ;Clean up
001A79 0F5A79 MACRO                   NUM_REVERSE_4           SSTACK_PREPULL  18
                                      ;#Check stack before pull operation                                              (SSTACK_PREPULL)
                                      ; args:   1: expecteded stack content (bytes)                                    (SSTACK_PREPULL)
                                      ; result: none                                                                   (SSTACK_PREPULL)
                                      ; SSTACK: none                                                                   (SSTACK_PREPULL)
                                      ;         X, Y, and D are preserved                                              (SSTACK_PREPULL)
001A79 0F5A79 MACRO                                           SSTACK_CHECK_BOUNDARIES 0, \1                            (SSTACK_PREPULL)
                                      ;############################################################################### (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
                                      ;# Macros                                                                      # (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
                                      ;############################################################################### (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
                                      ;#Initialization (initialization done by ISTACK module)                          (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
                                      ;#Check stack boundaries                                                         (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
                                      ; args:   1: required stack capacity (bytes)                                     (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
                                      ;         2: expected stack content  (bytes)                                     (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
                                      ; result: none                                                                   (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
                                      ; SSTACK: none                                                                   (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
                                      ;         X, Y, and D are preserved                                              (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
001A79 0F5A79 8F 1B 0A                                        CPS     #SSTACK_TOP+\1          ;=> 2 cycles     3 bytes (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
001A7C 0F5A7C 18 25 F9 E2                                     BLO     OF                      ;=> 3 cycles     4 bytes (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
001A80 0F5A80 8F 1B 13                                        CPS     #SSTACK_BOTTOM-\2       ;=> 2 cycles     3 bytes (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
001A83 0F5A83 18 22 F9 E1                                     BHI     UF                      ;=> 3 cycles     4 bytes (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
                                                                                              ;  ---------    -------- (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
                                                                                              ;  10 cycles    14 bytes (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
001A87 0F5A87 -> $1468                UF                      EQU     SSTACK_UF                                        (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
001A87 0F5A87 -> $1462                OF                      EQU     SSTACK_OF                                        (SSTACK_PREPULL/SSTACK_CHECK_BOUNDARIES)
001A87 0F5A87 1B 84                                           LEAS    4,SP                    ;release temporary space for forward number
001A89 0F5A89 3A                                              PULD                            ;
001A8A 0F5A8A 31                                              PULY
001A8B 0F5A8B 30                                              PULX
                                                              ;Done
001A8C 0F5A8C 3D                                              RTS
                                      ;#Print a reserse number digit - non-blocking
                                      ; args:   B:    base (2<=base<=16)
                                      ;         SP+0: MSB
                                      ;         SP+1:  |
                                      ;         SP+2:  |reverse
                                      ;         SP+3:  |number
                                      ;         SP+4:  |
                                      ;         SP+5: LSB
                                      ; result: SP+0: MSB
                                      ;         SP+1:  |remaining
                                      ;         SP+2:  | digits of
                                      ;         SP+3:  |reverse
                                      ;         SP+4:  |number
                                      ;         SP+5: LSB
                                      ;         C-flag: set if successful
                                      ; SSTACK: 8 bytes
                                      ;         X, Y and D are preserved
001A8D 0F5A8D -> $1A8D                NUM_REVPRINT_NB EQU     *
                                      ;Stack layout:
001A8D 0F5A8D -> $0000                NUM_REVPRINT_NB_COUNT   EQU     $00 ;SP+ 0: A
001A8D 0F5A8D -> $0001                NUM_REVPRINT_NB_BASE    EQU     $01 ;SP+ 1: base -> B
