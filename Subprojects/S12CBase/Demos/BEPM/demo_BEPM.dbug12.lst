                                                                          ;SP+ 3: +
001A8D 0F5A8D -> $0004                NUM_REVPRINT_NB_X       EQU     $04 ;SP+ 4: +X
                                                                          ;SP+ 5: +
001A8D 0F5A8D -> $0006                NUM_REVPRINT_NB_RTN     EQU     $06 ;SP+ 6: +return address
                                                                          ;SP+ 7: +
001A8D 0F5A8D -> $0008                NUM_REVPRINT_NB_RHW     EQU     $08 ;SP+ 8: MSB
                                                                          ;SP+ 9:  |copy
001A8D 0F5A8D -> $000A                NUM_REVPRINT_NB_RMW     EQU     $0A ;SP+10:  |of
                                                                          ;SP+11:  |reverse
001A8D 0F5A8D -> $000C                NUM_REVPRINT_NB_RLW     EQU     $0C ;SP+12:  |number
