---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>AriCalculator</title>
    <link rel="self" type="application/atom+xml" href="{{ site.url }}/AriCalculator.atom"/>
    <link rel="alternate" type="text/html" href="{{ site.url }}" />
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>{{ site.url }}</id>
    <author>
        <name>Dirk Heisswolf</name>
    </author>
    <rights>Copyright (c) 2010-2016 Dirk Heisswolf</rights>
    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="{{ site.url }}{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>{{ site.url }}{{ post.url }}</id>
        {% comment %}<summary type="html">{{ post.excerpt | xml_escape }}</summary>{% endcomment %}
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}
</feed>